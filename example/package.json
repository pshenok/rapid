{
  "name": "processinggateway",
  "version": "1.0.0",
  "description": "ProcessingGateway",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "start:dev": "node -r ts-node/register -r dotenv/config src/index.ts",
    "start:w": "nodemon --watch src --ext ts,json --exec 'yarn start:dev'",
    "clear": "rm -rf dist",
    "test:unit": "jest --config jest.config.json",
    "test:e2e": "jest --runInBand --config jest.config.e2e.json",
    "test:w": "jest --watch",
    "coverage": "jest --coverage",
    "lint": "eslint --quiet --ext ts,js .",
    "lint:fix": "eslint --fix --ext .ts .",
    "migrate:up": "node node_modules/.bin/sequelize db:migrate",
    "migrate:down": "node node_modules/.bin/sequelize db:migrate:undo",
    "migrate:status": "node node_modules/.bin/sequelize db:migrate:status",
    "migrate:dev:up": "node -r dotenv/config node_modules/.bin/sequelize db:migrate",
    "migrate:dev:down": "node -r dotenv/config node_modules/.bin/sequelize db:migrate:undo",
    "migrate:dev:status": "node -r dotenv/config node_modules/.bin/sequelize db:migrate:status"
  },
  "author": "Kostyantyn Pshenychnyy",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "awilix": "^4.2.2",
    "axios": "^0.19.0",
    "bcrypt": "^3.0.6",
    "cls-hooked": "^4.2.2",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "helmet": "^3.16.0",
    "joi": "^14.3.1",
    "joi-to-swagger": "^3.1.0",
    "json-stringify-safe": "^5.0.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.11",
    "log4js": "5",
    "moment": "^2.24.0",
    "pg": "^8.5.1",
    "randomstring": "^1.1.5",
    "reflect-metadata": "^0.1.13",
    "require-dir": "^1.2.0",
    "sequelize": "^6.3.5",
    "sequelize-cli": "^5.5.0",
    "swagger-ui-express": "^4.0.2",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/bcrypt": "^3.0.0",
    "@types/cls-hooked": "^4.3.0",
    "@types/cors": "^2.8.5",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.16.1",
    "@types/glob": "^7.1.1",
    "@types/helmet": "^0.0.43",
    "@types/jest": "^24.0.13",
    "@types/joi": "^14.3.3",
    "@types/json-stringify-safe": "^5.0.0",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/lodash": "^4.14.133",
    "@types/log4js": "^2.3.5",
    "@types/moment": "^2.13.0",
    "@types/node": "^12.0.2",
    "@types/randomstring": "^1.1.6",
    "@types/reflect-metadata": "^0.1.0",
    "@types/require-dir": "^1.0.0",
    "@types/sequelize": "^4.28.4",
    "@types/sinon": "^7.0.13",
    "@types/swagger-ui-express": "^3.0.0",
    "@types/uuid": "^3.4.4",
    "@typescript-eslint/eslint-plugin": "^2.1.0",
    "@typescript-eslint/parser": "^2.1.0",
    "dotenv": "^8.0.0",
    "eslint": "^6.3.0",
    "jest": "^24.8.0",
    "nodemon": "^1.18.10",
    "sinon": "^7.4.2",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.2.0",
    "typescript": "^3.4.5"
  }
}
